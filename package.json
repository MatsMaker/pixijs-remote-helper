{
  "name": "pixijs-remote-helper",
  "version": "1.0.0",
  "description": "",
  "bin": {
    "pixihelper": "./bin/pixihelper"
  },
  "scripts": {
    "babel": "babel --presets es2015 js/main.js",
    "app": "electron ./app",
    "app:dev": "webpack --mode development --config webpack.app.config.js & electron ./app",
    "app:dev:brk": "webpack --mode development --config webpack.app.config.js & electron --inspect-brk ./app",
    "server:dev": "nodemon --config nodemon.json server/index.js",
    "web:dev": "webpack-dev-server --mode development --config webpack.client.config.js",
    "web:prod": "webpack --mode production -p --progress --config webpack.client.config.js",
    "mixin:dev": "webpack-dev-server --mode development --config webpack.mixin.config.js",
    "mixin:prod": "webpack --mode production -p --progress --config webpack.mixin.config.js",
    "build": "npm run web:prod && npm run mixin:prod",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MatsMaker/pixijs-remote-helper.git"
  },
  "keywords": [
    "pixijs",
    "pixi",
    "devtools",
    "phaser",
    "phaserjs",
    "remoute",
    "helper",
    "tools",
    "debugger"
  ],
  "author": "Matsitsky Sergey",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "electron": "^7.2.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.1",
    "rxjs": "^6.3.3",
    "sass-loader": "^7.1.0",
    "script-loader": "^0.7.2",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.1",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "pixi.js": "^5.0.3",
    "socket.io": "^2.1.1",
    "socket.io-emitter": "^3.1.1",
    "vue": "^2.6.11"
  },
  "browserslist": "> 1%, not dead"
}
